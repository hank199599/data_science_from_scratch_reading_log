    {
      "cell_type": "code",
      "metadata": {
        "id": "ZyioivJmK5ze"
      },
      "source": [
        "interest_user_matrix = [[user_interest_vector[j] for user_interest_vector in user_interest_vectors] for j,_ in enumerate(unique_interests)]\r\n",
        "# print(interest_user_matrix)"
      ],
